<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link rel="icon" href="/images/window-logo.webp" type="image/webp" />
  <title>Develop Store | E-Commerce | Miguel Zacca</title>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link 
    href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500&display=swap" 
    rel="stylesheet" 
  />
  <% if (typeof cssFiles !== "undefined") { %>
    <% for (const file of cssFiles) { %>
      <link rel="stylesheet" href="/styles/<%= file %>" />
    <% } %>
  <% } %>
  <% if (typeof jsFiles !== "undefined") { %> 
    <% for (const file of jsFiles) { %>
      <script src="/script/<%= file %>" type="module" defer></script>
    <% } %>
  <% } %>
</head>
