{
  "name": "developstore",
  "version": "latest",
  "description": "Node.js authentication with Sequelize and MySQL.",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "build:ts": "npx tsc",
    "copy:public:bash": "cp -r ./src/public/images ./dist/public && cp -r ./src/public/css ./dist/public",
    "copy:public:cmd": "xcopy /E /I ./src/public/images ./dist/public && xcopy /E /I ./src/public/css ./dist/public",
    "copy:public": "copyfiles -u 2 './src/public/**/!(*.mts|*.ts)' ./dist/public",
    "start": "node dist/app.js",
    "build": "npm run build:ts && npm run copy:public"
  },
  "author": "Miguel Zacca",
  "license": "CC-BY-NC-4.0",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "sequelize": "^6.37.3",
    "sqlite3": "^5.1.7",
    "xss": "^1.0.15",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/sequelize": "^4.28.20",
    "copyfiles": "^2.4.1",
    "typescript": "^5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miguelzacca/e-commerce.git"
  },
  "bugs": {
    "url": "https://github.com/miguelzacca/e-commerce/issues"
  },
  "homepage": "https://github.com/miguelzacca/e-commerce#readme"
}
